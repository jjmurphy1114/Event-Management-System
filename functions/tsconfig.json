{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
    "lib": [
      "es2020",
    ],
    "resolveJsonModule": true,
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules"
  ]
}
