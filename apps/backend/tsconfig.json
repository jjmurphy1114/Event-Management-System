{
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "node_modules/@types",
    "src"
  ],
  "ts-node": {
    "swc": true
  }
}